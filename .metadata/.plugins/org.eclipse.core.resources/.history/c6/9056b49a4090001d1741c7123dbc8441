
## local oracle
ds.driverClassName = oracle.jdbc.driver.OracleDriver
ds.url = jdbc:oracle:thin:@localhost:1521:xe
ds.username = inventory
ds.password = inventory

## local mysql
mydb.driverClassName = net.sf.log4jdbc.sql.jdbcapi.DriverSpy
mydb.url = jdbc:log4jdbc:mysql://localhost:3306/spring
mydb.username = myspring
mydb.password = myspring
